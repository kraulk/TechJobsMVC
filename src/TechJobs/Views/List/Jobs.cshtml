<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>


    // TODO #3 - Use the same list display from Views/Search/Index.cshtml
    @if (ViewBag.jobs != null)
    {
        @foreach (Dictionary<string, string> result in ViewBag.jobs)
        {
            <table class="job-listing">
                @foreach (KeyValuePair<string, string> job in result)
                {
                    <tr>
                        <th>@job.Key</th>
                        <td>@job.Value</td>
                    </tr>
                }
            </table>
        }
    }
